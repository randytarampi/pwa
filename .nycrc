{
  "extension": [
    ".jsx"
  ],
  "exclude": [
    "node_modules/**",
    "coverage/**",
    ".nyc_output/**",
    "test/**",
    "*.js"
  ],
  "reporter": [
    "text-summary",
    "lcov"
  ],
  "cache": true,
  "all": true,
  "check-coverage": false,
  "sourceMap": true,
  "instrument": true,
  "watermarks": {
    "lines": [
      70,
      90
    ],
    "functions": [
      70,
      90
    ],
    "branches": [
      70,
      90
    ],
    "statements": [
      70,
      90
    ]
  }
}
